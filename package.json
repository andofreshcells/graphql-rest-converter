{
  "name": "openapi-graphql-bridge",
  "version": "1.0.0",
  "dependencies": {
    "ajv": "^8.11.0",
    "express": "^4.18.1",
    "graphql": "^16.2.0",
    "graphql-request": "^4.2.0",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "openapi-jsonschema-parameters": "^11.0.0",
    "openapi-request-coercer": "^11.0.0",
    "openapi-request-validator": "^11.0.0",
    "openapi-response-validator": "^11.0.0",
    "openapi-schema-validator": "^11.0.0",
    "type-fest": "^2.12.2"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && cpx \"./src/**/*.{csv,json}\" dist/src",
    "test": "cross-env JEST_JUNIT_OUTPUT_NAME=\"reports/junit/js-test-results.xml\" jest --coverage --ci --reporters=default --reporters=jest-junit ",
    "test:watch": "jest --watch",
    "prepack": "yarn build",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/git": "^10.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.177",
    "@types/node": "^16.11.7",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.3",
    "expect": "^27.5.1",
    "jest": "^27.5.1",
    "jest-junit": "^13.0.0",
    "openapi-types": "^10.0.0",
    "semantic-release": "^18.0.1",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.6.3"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "."
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.[jt]s?(x)"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,ts,tsx}"
    ]
  }
}
